<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	
	.header {
		font-weight: bold;
		font-size: medium;
		color: blue;
		margin-top: 1em;
		cursor: pointer;
	}
	
	.subHeader {
		font-weight: normal;
		font-size: small;
		color: red;
		margin: 0.5em;
	}
	</ui:style>
	<g:VerticalPanel>

		<g:Label ui:field="gameHeader" styleName="{style.header}"/>
		<g:Label ui:field="gameSubHeader" styleName="{style.subHeader}"/>

	</g:VerticalPanel>
</ui:UiBinder> 