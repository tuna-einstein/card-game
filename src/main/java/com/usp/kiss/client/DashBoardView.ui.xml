<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
		font-size: x-large;
	}
	
	.container {
		font-weight: bold;
		font-size: medium;
		margin: 1em;
	}
	
	.header {
		font-weight: bold;
		font-size: large;
		text-align: center;
	}
	
	.image {
		width: 5em;
	}
	
	.myTitle {
		margin: 1em;
	}
	</ui:style>
	<g:VerticalPanel width="100%" horizontalAlignment="ALIGN_CENTER">
		
		<g:VerticalPanel ui:field="container" width="100%"
			styleName="{style.container}" horizontalAlignment="ALIGN_CENTER" />
		<g:Image url="plus.png" ui:field="imagePlus" styleName="{style.image}"></g:Image>
	</g:VerticalPanel>
</ui:UiBinder> 